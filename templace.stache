<ul>
  {{#each components}}
    <li><a href="#!{{.}}">{{.}}</a>
  {{/each}}
</ul>
<div class="demo-area">
    <can-router>
        {{#each components}}
            <can-router-route href="{.}">
                <common-component-iframe component="{.}">
                </common-component-iframe>
            </can-router-route>
        {{/each}}
    </can-router>
</div>
